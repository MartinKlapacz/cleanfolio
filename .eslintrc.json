{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended"
  ],
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "require-jsdoc": 0,
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "off"
  }
}
